CREATE TABLE ALUNO 
( 
 MATRÍCULA INT PRIMARY KEY,  
 NOME INT,  
 DATA_NASC INT,  
 Atributo INT,  
); 

CREATE TABLE DISCIPLINA 
( 
 DISCIPLINA_ID INT PRIMARY KEY,  
 CÓDIGO INT,  
 DESCRICAO INT,  
 Atributo INT,  
); 

CREATE TABLE CURSA 
( 
 DISCIPLINA_ID INT PRIMARY KEY,  
 MATRÍCULA INT,  
); 

ALTER TABLE CURSA ADD FOREIGN KEY(DISCIPLINA_ID) REFERENCES DISCIPLINA (DISCIPLINA_ID)
ALTER TABLE CURSA ADD FOREIGN KEY(MATRÍCULA) REFERENCES ALUNO (MATRÍCULA)
